<h2>Available drones</h2>
<% allDrones.forEach(drone => { %>
    <ul style="width: 200px;">
        <li><%=drone.name%></li>
        <li><%=drone.propellers%></li>
        <li><%=drone.maxSpeed%></li>
        <li><a href="/drones/<%-drone._id%>/edit">Edit Drone</a></li>
    </ul>
    <form action="/drones/<%-drone._id%>/delete" method="POST" style="margin-bottom: 50px;">
        <button type="submit">Delete Drone</button>
    </form>
<% }) %>